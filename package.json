{
  "name": "ooad-ai",
  "type": "module",
  "private": true,
  "description": "OOAD AI Monorepo - Slidev presentation and system implementations",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "dev:slidev": "pnpm --filter slidev-presentation dev",
    "dev:zoo": "pnpm --filter zoo-system dev",
    "dev:aotter": "pnpm --filter aotter-wow-system dev",
    "build:slidev": "pnpm --filter slidev-presentation build",
    "build:zoo": "pnpm --filter zoo-system build",
    "build:aotter": "pnpm --filter aotter-wow-system build",
    "export:slidev": "pnpm --filter slidev-presentation export",
    "clean": "pnpm -r clean",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "prettier": "^3.0.3"
  },
  "packageManager": "pnpm@9.5.0+sha512.140036830124618d624a2187b50d04289d5a087f326c9edfc0ccd733d76c4f52c3a313d4fc148794a2a9d81553016004e6742e8cf850670268a7387fc220c903"
}
